
#include <stdio.h>

int main() {
int idade;
    printf("Digite sua idade: ");
    scanf(" %d", &idade);
    
    while(idade < 0 || idade > 130){
        printf("digite uma idade vÃ¡lida seu leproso: ");
        scanf(" %d", &idade);
    }
    
    if(idade >= 65){
        printf("buteco");
